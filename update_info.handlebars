{
  "swagger": "2.0",
  "info": {
    "version": "{{version}}",
    "title": "{{title}}"
  },
  "paths": {
    "/update_info": {
      "put": {
        "summary": "Change login credentials object by Id",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "firstName": {
                    "type": "string",
                    "default": "{{firstName}}"
                  },
                  "lastName": {
                    "type": "string",
                    "default": "{{lastName}}"
                  },
                  "country": {
                    "type": "string",
                    "default": "{{country}}"
                  },
                  "password": {
                    "type": "string",
                    "default": "{{password}}"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Names changed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/update_data"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    }
  }
}